{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/shop/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/shop/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/shop",
  "projects": {
    "shop": {
      "type": "application",
      "root": "apps/shop",
      "entryFile": "main",
      "sourceRoot": "apps/shop/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shop/tsconfig.app.json"
      }
    }
  }
}
